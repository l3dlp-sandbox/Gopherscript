require {
    read: {
        globals: "*"
        : https://*
        : /home/user/...
    }
    use: {
        globals: "*"
        : https://*
        commands: {
            echo: {}
            go: {
                help: {
                    build: {} 
                    run: {}
                }
            }
        }
    },
    create: {
        globals: "*"
        : https://*
        : https://localhost:8080
        : /home/user/...
    }
    update: {
        globals: "*"
        : https://*
        : https://localhost:8080
        : /home/user/...
    }
    delete: https://*
    provide: https://*
    limits: {
        "fs/read": 100kB/s
        "fs/write": 100kB/s
        "fs/total-new-file": 100000
        "fs/new-file": 10x/s

        "http/request": 1x/s

    }
}

@loc = https://localhost

return {
    builtin-commands: [cd, pwd]
    aliased-commands: [echo, go] # TODO: specify timeout durations ?
    prompt: [
        ["[" bright-green]
        [@(pwd()) bright-black]
        ["]> " bright-green]
    ]
}