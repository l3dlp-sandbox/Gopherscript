const (
    URL = https://pkg.go.dev/
)

require {
    read: {
        : $$URL
        globals: "*"
    }
    create: {
        : /home/user/...
    }
    limits: {
        "fs/write": 1MB/s
    }
    use: {globals: "*"}
}

result = wnav.do(
    navigate, $$URL
    screenshot, s!img.Homepage-logo!, "screenshot"
)!

create ./img.png $result.screenshot